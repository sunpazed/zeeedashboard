{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "react/style-prop-object": 0,
    "react/no-unused-prop-types": 0,
    "import/no-unresolved": [2, { "ignore": ["^[~@]"] }],
  }
}
